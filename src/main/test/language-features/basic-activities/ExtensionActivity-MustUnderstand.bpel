<?xml version="1.0" encoding="UTF-8"?>
<process
        name="ExtensionActivity-MustUnderstand"
        targetNamespace="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/bpel/extensionActivityMustUnderstand"
        xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
        xmlns:ti="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/wsdl/testinterface"
        xmlns:ex="http://dsg.wiai.uniba.de/bpel-engine-comparison/extension">
    <extensions>
        <extension namespace="http://dsg.wiai.uniba.de/bpel-engine-comparison/extension" mustUnderstand="yes">
        </extension>
    </extensions>
    <import namespace="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/wsdl/testinterface"
            location="../TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="InitData" messageType="ti:executeProcessSyncRequest"/>
    </variables>
    <sequence>
        <receive name="InitialReceive" createInstance="yes" partnerLink="MyRoleLink" operation="startProcessSync"
                 portType="ti:TestInterfacePortType" variable="InitData"/>
        <extensionActivity>
            <ex:foo></ex:foo>
        </extensionActivity>
        <assign name="AssignReplyData">
            <copy>
                <from variable="InitData" part="inputPart"/>
                <to variable="ReplyData" part="outputPart"/>
            </copy>
        </assign>
        <reply name="ReplyToInitialReceive" partnerLink="MyRoleLink" operation="startProcessSync"
               portType="ti:TestInterfacePortType" variable="ReplyData"/>
    </sequence>
</process>